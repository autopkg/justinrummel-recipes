<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Download recipe for VMware Fusion Guest Tools</string>
	<key>Identifier</key>
	<string>com.justinrummel.download.VMwareFusionGuestTools</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>com.vmware.fusionGuestTools</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.2.5</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Processor</key>
			<string>VMwareFusionURLProvider</string>
			<key>Arguments</key>
			<dict>
				<key>product_name</key>
				<string>fusion.xml</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>URLDownloader</string>
			<key>Arguments</key>
			<dict>
				<key>filename</key>
				<string>%NAME%.zip.tar</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>